package lotto;

import java.util.List;

/*
 * 로또 한장을 의미하는 객체
 */
public class Lotto {
	private final List<Integer> numbers;
	
	public Lotto(List<Integer> numbers) {
		this.numbers = numbers; 
	}
	
	public void showLottoNumber() {
		System.out.println(numbers.stream().mapToInt(i -> i.intValue()).toArray());
	}
	
}
