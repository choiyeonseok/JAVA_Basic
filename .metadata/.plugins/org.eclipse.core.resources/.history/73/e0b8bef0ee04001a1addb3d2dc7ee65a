package paractice;

public abstract class PlayerLevel{

	
	public abstract void run() {};
	public abstract void jump() {};
	public abstract void turn() {};
	public abstract void showLevelMessage() {};
	
	//final로 선언 시 시나리오 변경 불가 
	final public void go(int n) {
		run();
		for (int i=0; i<n; i++) {
			jump();
		}
		turn();
	}
	
}
