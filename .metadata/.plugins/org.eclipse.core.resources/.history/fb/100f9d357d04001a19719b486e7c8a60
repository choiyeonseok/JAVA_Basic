package inheritance;

public class VIPCustomer extends Customer{

	double salesRatio;
	private int agentID;
	
	public VIPCustomer(int customerID, String customerName) {
		
		// super()  <= 넣든 안넣든 컴파일러에서 자동으로 상위 클래스 생성자를 호출한다.
		super(customerID, customerName);
		customerGrade = "VIP";
		bonusRatio = 0.05;
		salesRatio = 0.1;
		
	}

	
}
