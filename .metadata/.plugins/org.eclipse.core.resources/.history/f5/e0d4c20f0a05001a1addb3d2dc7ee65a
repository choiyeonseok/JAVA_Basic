package paractice;

public class AdvancedLevel extends PlayerLevel{

	public void run() {
		System.out.println("빠르게 달립니다.");
	}
	
	public void jump() {
		System.out.println("jump 할수 있다.");
	}
	
	public void turn() {
		System.out.println("turn 못하지롱");
	}
	
	public void showPlayerLevel() {
		System.out.println("****** 중급자 레벨입니다. ******");
	}
	
}
