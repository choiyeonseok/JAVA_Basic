package practice2;

public class Coffee {

	public String name; //별다방, 콩다방
	public int money = 0;
	
	
	public Coffee(String name){
		this.name = name;
	}
	
	public void buy(int price) {
		this.money += price;
	}
	
	public void showInfo() {
		System.out.println(name + "의  수입은  총" + money + "입니다.");
	}
	
	
	
	
	
}
